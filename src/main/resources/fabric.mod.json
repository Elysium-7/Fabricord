{
	"schemaVersion": 1,
	"id": "fabricord",
	"version": "${version}",
	"name": "Fabricord",
	"description": "Modern and simple Discord Bridge Mod for FabricServer.",
	"authors": [
		"Ruxy"
	],
	"license": "MIT",
	"icon": "assets/fabricord/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.inf_ruxy.projects.mc.plugin.several.fabricord.Fabricord",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "com.inf_ruxy.projects.mc.plugin.several.fabricord.FabricordDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"fabricord.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.6",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.9.22"
	},
	"suggests": {
		"another-mod": "*"
	}
}