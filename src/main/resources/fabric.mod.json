{
	"schemaVersion": 1,
	"id": "fabricord",
	"version": "${version}",
	"name": "Fabricord",
	"description": "A modern message style like DiscordSRV will be reproduced as a Fabric version mod.",
	"contributors": [
		"Lars"
	],
	"contact": {
		"sources": "https://github.com/K-Lqrs/Fabricord"
	},
	"license": "Apache-2.0",
	"icon": "assets/fabricord/icon.png",
	"environment": "server",
	"entrypoints": {
		"server": [
			{
				"adapter": "kotlin",
				"value": "net.ririfa.fabricord.Fabricord"
			}
		]
	},
	"mixins": [
		"fabricord.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": ">=1.21.4",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.12.0+kotlin.2.0.10"
	}
}